{
    "name": "xahmedtaha/paybridge",
    "description": "A versatile laravel package providing a consistent and easy-to-use interface for integrating with multiple payment gateways.",
    "keywords": [
        "Ahmed Taha",
        "laravel",
        "paybridge"
    ],
    "homepage": "https://github.com/xahmedtaha/paybridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Taha",
            "email": "xahmedtaha930@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/invade": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "AhmedTaha\\PayBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmedTaha\\PayBridge\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-paybridge --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedTaha\\PayBridge\\PayBridgeServiceProvider"
            ],
            "aliases": {
                "PayBridge": "AhmedTaha\\PayBridge\\Facades\\PayBridge"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
